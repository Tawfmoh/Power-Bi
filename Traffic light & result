# GM % CY = 
   VAR GM_CY = DIVIDE(
        [Total Profit CY],
        [Total Sales CY]
   
     )
RETURN

SWITCH(
    true(),
    GM_CY<0.3,
    "ðŸ”´ " & format(GM_CY,"0.0%"),
    GM_CY<0.385,
    "ðŸŸ¡ " & format(GM_CY,"0.0%"),
    GM_CY>=0.385,
    "ðŸŸ¢ " & format(GM_CY,"0.0%")
)

